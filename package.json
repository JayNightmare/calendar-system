{
  "name": "calendar-system",
  "version": "1.0.0",
  "private": true,
  "description": "A React based calendar website with reminders, plans and events. It supports colour coded events, filters, search and subtle zoom animations when switching between calendar views. The project is ready to be deployed on GitHub Pages.",
  "homepage": "https://jaynightmare.github.io/calendar-system",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-big-calendar": "^1.5.1",
    "date-fns": "^2.30.0",
    "gh-pages": "^5.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}